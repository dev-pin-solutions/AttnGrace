{% comment %}
  @param item Product
         The product to display.

  @param placeholder String
         If set, use a Shopify placeholder SVG as the product image.

  @param title String
         The product title to use. Only used if no `item` is passed.
{% endcomment %}

{% assign image_size = '512x' %}
{% assign placeholder_style = placeholder | default: 'image' %}
{% assign crop_image = null %}
{% if section.settings.product_grid_image_crop %}
  {% assign crop_image = 'center' %}
{% endif %}

{% if item.featured_media %}
  {%- capture background_style -%}
    {%
      include 'rimg-home',
      img: item.featured_media.preview_image,
      size: image_size,
      crop: crop_image,
      lazy: true,
      background: true
    %}
  {%- endcapture -%}
{% else %}
  {%- capture placeholder_uri -%}
    {%-
      include 'placeholder_svg_uri',
      placeholder: placeholder_style
    -%}
  {%- endcapture -%}

  {%- capture background_style -%}
    style="background-image: url('{{ placeholder_uri }}');"
  {%- endcapture -%}
{% endif %}

<div class="recommended-product">
  {% include 'product-badge' %}
    <div class="product-list-item-thumbnail">
        <div class="product-image-carousel">
            <a href="{{ product.url }}"><img src="" alt="" /></a>
            <button class="carousel-prev">
                <svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" width="20" height="20" viewBox="0 0 20 20" fill="none">
                    <rect x="20" y="20" width="20" height="20" transform="rotate(-180 20 20)" fill="url(#pattern0_404_1305)"/>
                    <defs>
                        <pattern id="pattern0_404_1305" patternContentUnits="objectBoundingBox" width="1" height="1">
                            <use xlink:href="#image0_404_1305" transform="scale(0.00195312)"/>
                        </pattern>
                        <image id="image0_404_1305" width="512" height="512" xlink:href="data:image/png;base64,"/>
                    </defs>
                </svg>
            </button>
            <button class="carousel-next">
                <svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" width="20" height="20" viewBox="0 0 20 20" fill="none">
                    <rect width="20" height="20" fill="url(#pattern0_404_1307)"/>
                    <rect width="20" height="20" fill="url(#pattern1_404_1307)"/>
                    <rect width="20" height="20" fill="url(#pattern2_404_1307)"/>
                    <defs>
                        <pattern id="pattern0_404_1307" patternContentUnits="objectBoundingBox" width="1" height="1">
                            <use xlink:href="#image0_404_1307" transform="scale(0.00195312)"/>
                        </pattern>
                        <pattern id="pattern1_404_1307" patternContentUnits="objectBoundingBox" width="1" height="1">
                            <use xlink:href="#image0_404_1307" transform="scale(0.00195312)"/>
                        </pattern>
                        <pattern id="pattern2_404_1307" patternContentUnits="objectBoundingBox" width="1" height="1">
                            <use xlink:href="#image0_404_1307" transform="scale(0.00195312)"/>
                        </pattern>
                        <image id="image0_404_1307" width="512" height="512" xlink:href="data:image/png;base64,"/>
                    </defs>
                </svg>
            </button>
        </div>
        <div class="carousel-dots">
            {% for media in item.media %}
                {% if media.media_type == 'image' %}
                    <button class="dot"></button>
                {% endif %}
            {% endfor %}
        </div>
        <div class="carousel-hidden">
            {% for media in item.media %}
                {% if media.media_type == 'image' %}
                    <div class="carousel-slide">
                        <img src="{{ media | img_url: 'large' }}" alt="{{ item.title | escape }}">
                    </div>
                {% endif %}
            {% endfor %}
        </div>
    </div>
    {% if template contains 'search' %}
    <div class="search-item-product-details">
      <span class="search-item-product-title">
        {% if item %}
          {{ item.title }}
        {% else %}
          {{ title }}
        {% endif %}
      </span>
      {% if item.available %}
        <span class="search-item-product-price">
          {% if item.price_varies %}{{ 'products.product.from' | t }}{% endif %}
          <span data-money>{{ item.price_min | money }}</span>
        </span>
      {% endif %}
    </div>
  {% else %}
    <div class="recommended-product-desc {% if item.price %}has-price{% endif %}">
      <h3 class="recommended-product-title">
        <a href="{{ item.url | within: collection | default: '#' }}">
          {% if item %}
            {{ item.title }}
          {% else %}
            {{ title }}
          {% endif %}
        </a>
      </h3>
        <div class="widget_wrapper_fit">
            <a href="{{ item.url | within: collection | default: '#' }}#okendo-reviews-widget-wrapper" >
                {% include 'okendo-reviews-product-rating-summary', product: product %}

            </a>
            {% if product.metafields.custom.collection_popup_object != blank %}
                <button class="open-popup" data-popup="popup-{{ product.handle }}">
                    <svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" width="20" height="20" viewBox="0 0 20 20" fill="none">
                        <rect width="20" height="20" fill="url(#pattern0_404_146)"/>
                        <defs>
                            <pattern id="pattern0_404_146" patternContentUnits="objectBoundingBox" width="1" height="1">
                                <use xlink:href="#image0_404_146" transform="scale(0.00195312)"/>
                            </pattern>
                            <image id="image0_404_146" width="512" height="512" xlink:href="data:image/png;base64,"/>
                        </defs>
                    </svg>
                    <span>Fit & Sizing</span>
                </button>
            {% endif %}
        </div>



      {% comment %}
          <div class="recommended-short-desc">
            {{ product.metafields.global.short_description }}
          </div>
        {% else %}
          <div class="recommended-short-desc">
            {{ variant.metafields.global.short_description }}
          </div>
        {% endif %}
      {% endcomment %}

      {% include 'product-grid-form' %}
    </div>
  {% endif %}
</div>
{% if product.metafields.custom.collection_popup_object != blank %}
    {% assign object_popup =  product.metafields.custom.collection_popup_object.value %}
    <div id="popup-{{ product.handle }}" class="popup-modal">
        <div class="accordion_heading">
            <div class="popup-title">Fit & Sizing</div>
            <button class="close-popup">
                <svg xmlns="http://www.w3.org/2000/svg" width="10" height="10" viewBox="0 0 10 10" fill="none">
                    <path d="M9.677 8.118C9.77943 8.22037 9.8607 8.3419 9.91616 8.47568C9.97162 8.60945 10.0002 8.75284 10.0002 8.89765C10.0003 9.04246 9.9718 9.18586 9.91643 9.31967C9.86105 9.45348 9.77987 9.57507 9.6775 9.6775C9.57514 9.77993 9.4536 9.8612 9.31983 9.91666C9.18605 9.97212 9.04267 10.0007 8.89785 10.0007C8.75304 10.0008 8.60964 9.9723 8.47583 9.91693C8.34202 9.86155 8.22043 9.78037 8.118 9.678L5 6.558L1.882 9.677C1.77964 9.77943 1.6581 9.8607 1.52433 9.91616C1.39055 9.97162 1.24717 10.0002 1.10235 10.0002C0.957541 10.0003 0.814137 9.9718 0.68033 9.91643C0.546522 9.86105 0.424932 9.77987 0.322501 9.6775C0.115632 9.47076 -0.000638597 9.19032 -0.000732365 8.89785C-0.000826133 8.60539 0.115264 8.32487 0.322001 8.118L3.442 5L0.323001 1.882C0.116132 1.67526 -0.000138597 1.39482 -0.000232365 1.10235C-0.000326133 0.809891 0.115764 0.529369 0.322501 0.322501C0.529237 0.115632 0.809684 -0.000638597 1.10215 -0.000732365C1.39461 -0.000826133 1.67513 0.115264 1.882 0.322001L5 3.442L8.118 0.323001C8.22037 0.22057 8.3419 0.139304 8.47568 0.0838435C8.60945 0.0283831 8.75284 -0.000185951 8.89765 -0.000232382C9.04246 -0.000278812 9.18586 0.0281982 9.31967 0.0835729C9.45348 0.138948 9.57507 0.220135 9.6775 0.322501C9.77993 0.424866 9.8612 0.546404 9.91666 0.680176C9.97212 0.813948 10.0007 0.957334 10.0007 1.10215C10.0008 1.24696 9.9723 1.39036 9.91693 1.52417C9.86155 1.65798 9.78037 1.77957 9.678 1.882L6.558 5L9.676 8.118H9.677Z" fill="black" fill-opacity="0.3"/>
                </svg>
            </button>
        </div>
        <div class="popup-content">

            <h2>{{ product.title }}</h2>

            <div class="accordion">
                {% for value in product.metafields.custom.collection_popup_object.value %}
                <div class="accordion-item">
                    <button  {% if value.product_title_for_open_accordion == product.title %} class="active" {% endif %}>{{ value.accordion_title }}</button>
                    <div class="accordion-content {% if value.product_title_for_open_accordion == product.title %} visible first_acc {% endif %}">

                        {% assign number_of_drops = value.number_of_drops | default: 0 %} <!-- Берем число из переменной -->
                        {% assign max_drops = 7 %} <!-- Максимальное количество капель -->

                        <div class="drops-container">
                            {% for i in (1..max_drops) %}
                                {% if i <= number_of_drops %}
                                    <!-- SVG закрашенной капли -->
                                    <svg width="10" height="15" viewBox="0 0 10 15" fill="none" xmlns="http://www.w3.org/2000/svg">
                                        <path d="M9.61553 9.63576C9.61553 12.3219 7.53373 14.4709 5.00015 14.4709C2.46657 14.4709 0.384766 12.3219 0.384766 9.63576C0.384766 9.00376 0.66549 8.12076 1.13063 7.11264C1.5897 6.11765 2.2052 5.04882 2.82577 4.06159C3.44553 3.07564 4.06596 2.17802 4.53177 1.52651C4.71527 1.26987 4.87457 1.05169 5.00015 0.881653C5.12573 1.05169 5.28504 1.26987 5.46853 1.52651C5.93434 2.17802 6.55477 3.07564 7.17453 4.06159C7.79507 5.04882 8.41061 6.11765 8.86969 7.11264C9.33484 8.12076 9.61553 9.00376 9.61553 9.63576Z" fill="black" stroke="black" stroke-width="0.769231"/>
                                    </svg>
                                {% else %}
                                    <!-- SVG незакрашенной капли -->
                                    <svg width="11" height="15" viewBox="0 0 11 15" fill="none" xmlns="http://www.w3.org/2000/svg">
                                        <path d="M10.2308 9.45943C10.2308 12.1456 8.14897 14.2946 5.61538 14.2946C3.0818 14.2946 1 12.1456 1 9.45943C1 8.82743 1.28072 7.94443 1.74587 6.93631C2.20494 5.94132 2.82043 4.87249 3.44101 3.88525C4.06077 2.89931 4.68119 2.00169 5.14701 1.35018C5.3305 1.09354 5.48981 0.875362 5.61538 0.705322C5.74096 0.875362 5.90027 1.09354 6.08376 1.35018C6.54958 2.00169 7.17 2.89931 7.78976 3.88525C8.41031 4.87249 9.02585 5.94132 9.48492 6.93631C9.95008 7.94443 10.2308 8.82743 10.2308 9.45943Z" stroke="black" stroke-width="0.769231"/>
                                    </svg>
                                {% endif %}
                            {% endfor %}
                        </div>
                        <div class="accordion_with_img_block">
                            <img src="{{ value.icon_first | img_url: 'master' }}" alt="">
                            <span>
                                {{ value.text_first |  metafield_tag }}
                            </span>
                        </div>
                        <div class="accordion_with_img_block">
                            <img src="{{ value.icon_second | img_url: 'master' }}" alt="">
                            <span>
                                {{ value.text_second |  metafield_tag }}
                            </span>
                        </div>
                        <div class="accordion_with_img_block">
                            <img src="{{ value.icon_third | img_url: 'master' }}" alt="">
                            <span>
                                {{ value.text_third |  metafield_tag }}
                            </span>
                        </div>
                        <a href="{{ product.url }}">{{ value.button_text }}</a>
                    </div>
                </div>
                {% endfor %}

            </div>

            <a class="popup_last_btn" href="#quiz-29HrVVj">FIND YOUR FIT</a>
        </div>

    </div>
{% endif %}



